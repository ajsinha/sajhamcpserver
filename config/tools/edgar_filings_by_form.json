{
  "name": "edgar_filings_by_form",
  "implementation": "sajha.tools.impl.enhanced_edgar_tool.EDGARFilingsByFormTool",
  "description": "Search and filter company filings by specific form type (10-K, 10-Q, 8-K, DEF 14A, etc.) with optional date range filtering",
  "version": "2.2.0",
  "enabled": true,
  "inputSchema": {
    "type": "object",
    "properties": {
      "cik": {
        "type": "string",
        "description": "Central Index Key (CIK)"
      },
      "form_type": {
        "type": "string",
        "description": "SEC form type to filter",
        "enum": ["10-K", "10-Q", "8-K", "4", "3", "5", "S-1", "S-3", "S-4", "S-8", "DEF 14A", "13F-HR", "13F-NT", "20-F", "6-K", "40-F", "424B2", "424B3", "424B4", "424B5", "SC 13D", "SC 13G", "SC 13G/A", "144", "N-Q", "N-CSR", "N-PORT", "NPORT-P"]
      },
      "start_date": {
        "type": "string",
        "description": "Filter filings from this date onwards (YYYY-MM-DD format)"
      },
      "end_date": {
        "type": "string",
        "description": "Filter filings up to this date (YYYY-MM-DD format)"
      },
      "limit": {
        "type": "integer",
        "description": "Maximum number of filings to return (1-500)",
        "minimum": 1,
        "maximum": 500,
        "default": 50
      }
    },
    "required": ["cik", "form_type"]
  },
  "outputSchema": {
    "type": "object",
    "properties": {
      "cik": {
        "type": "string",
        "description": "Central Index Key"
      },
      "company_name": {
        "type": "string",
        "description": "Company name"
      },
      "form_type": {
        "type": "string",
        "description": "Form type filtered"
      },
      "form_description": {
        "type": "string",
        "description": "Description of form type"
      },
      "filings_count": {
        "type": "integer",
        "description": "Number of filings returned"
      },
      "filings": {
        "type": "array",
        "description": "List of matching filings with direct document URLs",
        "items": {
          "type": "object",
          "properties": {
            "accessionNumber": {
              "type": "string",
              "description": "Filing accession number"
            },
            "filingDate": {
              "type": "string",
              "description": "Date filed with SEC (YYYY-MM-DD)"
            },
            "reportDate": {
              "type": "string",
              "description": "Report period end date (YYYY-MM-DD)"
            },
            "form": {
              "type": "string",
              "description": "Form type"
            },
            "primaryDocument": {
              "type": "string",
              "description": "Primary document filename"
            },
            "primaryDocDescription": {
              "type": "string",
              "description": "Primary document description"
            },
            "document_url": {
              "type": "string",
              "description": "Direct URL to view the filing on SEC website"
            }
          }
        }
      }
    }
  },
  "metadata": {
    "author": "Ashutosh Sinha",
    "category": "SEC EDGAR",
    "tags": ["sec", "edgar", "filings", "10-K", "10-Q", "8-K", "forms", "annual report", "quarterly report"],
    "rateLimit": 10,
    "cacheTTL": 1800,
    "note": "Filtered view of company filings by form type. Useful for tracking specific disclosure types or regulatory requirements.",
    "examples": [
      {
        "description": "Get Apple's last 5 annual reports (10-K)",
        "input": {
          "cik": "320193",
          "form_type": "10-K",
          "limit": 5
        }
      },
      {
        "description": "Get Tesla's quarterly reports from 2023",
        "input": {
          "cik": "1318605",
          "form_type": "10-Q",
          "start_date": "2023-01-01",
          "end_date": "2023-12-31"
        }
      },
      {
        "description": "Get Microsoft's proxy statements",
        "input": {
          "cik": "789019",
          "form_type": "DEF 14A",
          "limit": 10
        }
      },
      {
        "description": "Get recent 8-K current reports",
        "input": {
          "cik": "320193",
          "form_type": "8-K",
          "limit": 20
        }
      }
    ],
    "useCases": [
      "Accessing annual and quarterly reports",
      "Tracking material events (8-K)",
      "Reviewing proxy statements",
      "Monitoring insider transactions",
      "Analyzing foreign issuer reports",
      "Following registration statements"
    ],
    "formTypes": {
      "10-K": "Annual Report - comprehensive financial information",
      "10-Q": "Quarterly Report - unaudited financial statements",
      "8-K": "Current Report - material corporate events",
      "DEF 14A": "Proxy Statement - shareholder voting information",
      "4": "Insider Trading - changes in beneficial ownership",
      "13F-HR": "Institutional Holdings - quarterly portfolio disclosure",
      "S-1": "IPO Registration - initial public offering",
      "20-F": "Annual Report - foreign private issuers",
      "SC 13D": "Beneficial Ownership - activist investor disclosure"
    }
  }
}
