{
  "name": "msdoc_get_word_metadata",
  "implementation": "sajha.tools.impl.msdoc_tools_tool_refactored.MsDocGetWordMetadataTool",
  "description": "Extract metadata and document properties from Word files",
  "version": "2.2.0",
  "enabled": true,
  "inputSchema": {
    "type": "object",
    "properties": {
      "filename": {
        "type": "string",
        "description": "Name of the Word file"
      }
    },
    "required": ["filename"]
  },
  "outputSchema": {
    "type": "object",
    "properties": {
      "filename": {"type": "string"},
      "metadata": {
        "type": "object",
        "description": "Document metadata",
        "properties": {
          "author": {"type": "string"},
          "title": {"type": "string"},
          "subject": {"type": "string"},
          "created": {"type": "string"},
          "modified": {"type": "string"}
        }
      }
    }
  },
  "metadata": {
    "author": "Ashutosh Sinha",
    "category": "Document Processing",
    "tags": ["word", "metadata", "properties"],
    "rateLimit": 60,
    "cacheTTL": 3600,
    "examples": [
      {
        "description": "Get Word document metadata",
        "input": {"filename": "contract.docx"}
      }
    ],
    "useCases": [
      "Document provenance tracking",
      "Metadata extraction",
      "Document property analysis"
    ]
  }
}
