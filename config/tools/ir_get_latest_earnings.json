{
  "name": "ir_get_latest_earnings",
  "implementation": "tools.impl.investor_relations_tool_refactored.IRGetLatestEarningsTool",
  "description": "Get the latest earnings report and presentation for a company",
  "version": "1.0.0",
  "enabled": true,
  "inputSchema": {
    "type": "object",
    "properties": {
      "ticker": {
        "type": "string",
        "description": "Stock ticker symbol (e.g., TSLA, MSFT)"
      }
    },
    "required": ["ticker"]
  },
  "outputSchema": {
    "type": "object",
    "properties": {
      "ticker": {"type": "string"},
      "latest_earnings": {
        "type": "object",
        "properties": {
          "quarter": {"type": "string", "description": "Quarter (e.g., Q3)"},
          "year": {"type": "integer"},
          "report_url": {"type": "string"},
          "presentation_url": {"type": "string"},
          "date": {"type": "string"}
        }
      },
      "success": {"type": "boolean"}
    }
  },
  "metadata": {
    "author": "Ashutosh Sinha",
    "category": "Financial Analysis",
    "tags": ["earnings", "quarterly results", "financial reports"],
    "rateLimit": 30,
    "cacheTTL": 3600,
    "supported_companies": ["TSLA", "MSFT", "C", "BMO", "RY", "JPM", "GS"],
    "examples": [
      {
        "description": "Get Tesla's latest earnings",
        "input": {"ticker": "TSLA"}
      },
      {
        "description": "Get JPMorgan's latest earnings",
        "input": {"ticker": "JPM"}
      }
    ],
    "useCases": [
      "Track latest quarterly results",
      "Access most recent earnings presentation",
      "Monitor company performance updates"
    ]
  }
}
